{
  "name": "devtool",
  "version": "0.0.0",
  "private": true,
  "author": "songhuabiao",
  "description": "development tool for electron",
  "main": "dist/electron/main.js",
  "scripts": {
    "dev": "vite --port=8080",
    "build": "vite build",
    "preview": "vite preview",
    "start": "electron-forge start",
    "copy:electron": "node build/copy.electron.js",
    "package": "npm run build && npm run copy:electron && electron-forge package --config=./forge.config.js",
    "make": "npm run build && npm run copy:electron && electron-forge make --config=./forge.config.js"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.1",
    "element-plus": "^2.11.8",
    "pinia": "^2.2.6",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.10.2",
    "@electron-forge/maker-deb": "^7.10.2",
    "@electron-forge/maker-rpm": "^7.10.2",
    "@electron-forge/maker-squirrel": "^7.10.2",
    "@electron-forge/maker-zip": "^7.10.2",
    "@electron-forge/plugin-auto-unpack-natives": "^7.10.2",
    "@electron-forge/plugin-fuses": "^7.10.2",
    "@electron/fuses": "^1.8.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "electron": "^33.2.1",
    "sass-embedded": "^1.93.3",
    "vite": "^5.4.10",
    "vite-plugin-vue-devtools": "^7.5.4"
  }
}
